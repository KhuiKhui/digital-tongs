generator client {
  provider = "prisma-client"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id         String   @id @default(uuid())
  username   String
  password   String
  chosenChar String?
  chars      String[]
  level      Int?     @default(1)
  exp        Int?     @default(0)
  funds      Int?     @default(1000)
}
